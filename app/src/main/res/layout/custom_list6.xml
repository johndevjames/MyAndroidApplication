<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" 
android:orientation="vertical"
 android:background="@drawable/edit_text_border2" 
 android:layout_width="wrap_content" 
 android:layout_height="wrap_content">

	<TextView
		android:id="@+id/label4"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentStart="true"
		android:layout_below="@+id/label3"
		android:layout_marginLeft="10dp"
		android:layout_toStartOf="@+id/button13"
		android:fontFamily="sans-serif-condensed"
		android:text="TextView"
		android:textAppearance="@style/TextAppearance.AppCompat.Medium" />

	<TextView
		android:id="@+id/label5"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignStart="@+id/label4"
		android:layout_below="@+id/label4"
		android:text="TextView"
		android:textAppearance="@style/TextAppearance.AppCompat.Small"
		android:layout_toStartOf="@+id/button13" />

	<Button
		android:id="@+id/button13"

		style="@style/Widget.AppCompat.Button.Small"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentEnd="true"
		android:layout_alignParentTop="true"
		android:background="@drawable/ic_action_navigation_close"
		android:focusable="false"
		android:focusableInTouchMode="false" />
</RelativeLayout>
